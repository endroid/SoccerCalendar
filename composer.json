{
    "name": "endroid/soccer-calendar",
    "description": "Endroid Soccer Calendar",
    "keywords": ["endroid", "soccer", "calendar", "bundle", "symfony"],
    "homepage": "https://github.com/endroid/SoccerCalendar",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-gd": "*",
        "endroid/calendar": "^1.0",
        "symfony/cache": "^2.7|^3.0|^4.0",
        "fabpot/goutte": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\SoccerCalendar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\SoccerCalendar\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
